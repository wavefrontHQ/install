{
  "_comment": {
      "standard_format": "see below",
      "APP_NAME": {
          "app_search": "search_name",
          "module": "python_module_name",
          "plugin_name": "collectd_plugin_name",
          "class_name": "installer_name",
          "conf_name": "plugin_conf_filename"
      }
  },
  "data": {
      "mysql": 
      {
          "app_search": "mysqld",
          "module": "mysql_plugin",
          "plugin_name": "mysql",
          "class_name": "MySQLInstaller",
          "conf_name": "wavefront_mysql.conf"
      },
      "apache":
      {
          "app_search": "apache2|httpd",
          "module": "apache_plugin",
          "plugin_name": "apache",
          "class_name": "ApacheInstaller",
          "conf_name": "wavefront_apache.conf"
      },
      "postgresql":
      {
          "app_search": "postgres",
          "module": "postgresql_plugin",
          "plugin_name": "postgresql",
          "class_name": "PostgresqlInstaller",
          "conf_name": "wavefront_postgres.conf"
      }
  }
}
